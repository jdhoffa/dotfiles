[user]
	name = Jackson Hoffart
	email = jackson.hoffart@gmail.com
[url "git@github.com:"]
	insteadOf = https://github.com/
[core]
 	editor = vim
 	excludesfile = /Users/jdhoffa/.gitignore
	pager = delta
[interactive]
	diffFilter = delta --color-only
[color]
	ui = true
[diff]
	colorMoved = default
[delta]
	navigate = true
	line-numbers = true
[merge]
	conflictstyle = diff3
	tool = vimdiff
[mergetool]
	path = nvim
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
